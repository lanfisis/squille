{
    "name": "burdz/squille",
    "description": "A toolsuite for PHP interactive performance profiling",
    "minimum-stability": "dev",
    "keywords": ["profiling", "performance"],
    "homepage": "https://github.com/lanfisis/squille",
    "license": "WTFPL",
    "authors": [
        {
            "name": "David Buros",
            "email": "david.buros@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "knplabs/gaufrette": "0.1.*",
        "evenement/evenement": "2.0.*",
        "perftools/xhgui": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": { "Burdz\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {"Burdz\\Test\\": "tests/units/"}
    },
    "scripts": {
        "test": "phpunit"
    }
}
